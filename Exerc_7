download.file(url, "banco.sav", mode = "wb")

banco <- read_sav("banco.sav")
write_sav(banco, "banco.sav")
banco


#Variável Apreciação Bolsonaro - Q1607
#Variável Idade - D1A_ID 
#Variável Educação - D3_ESCOLA
